    $(window).scroll(function () {
        var _videoTop = $("#ytplayer").offset().top - $(window).height() + 100;
        var _videoBottom = $("#ytplayer").offset().top + $("#ytplayer").height() - 100;
        if (($(window).scrollTop() > _videoTop) && ($(window).scrollTop() < _videoBottom)) {
            if ((playerState !== YT.PlayerState.PLAYING) && (reset === 1)) {
                player.playVideo();
                reset = 0;
            }
        }
        else {
            if (reset === 0) {
                player.pauseVideo();
                reset = 1;
            }
        }
    });

    function onPlayerReady(event) {
    }

    function onPlayerStateChange(event) {
        playerState = event.data;
    }

    $(document).ready(function () {
        $(".ui-mode").slick({
            slidesToShow: 1,
            slidesToScroll: 1,
            arrows: false,
            dots: true,
            infinite: true,
            speed: 600,
            autoplaySpeed: 4000,
            autoplay: false,
            pauseOnHover: false,
        });
    });
</script>

